{
    "name": "spryker-sdk/architector",
    "type": "library",
    "description": "Architector",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "rector/rector": "^0.11.42"
    },
    "require-dev": {
        "codeception/codeception": "^4.0.0",
        "phpunit/phpunit": "*",
        "spryker/code-sniffer": "*",
        "spryker/testify": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "SprykerSdk\\": "src/SprykerSdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerSdkTest\\": "tests/SprykerSdkTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml --ignore=/Fixture/,/tests/_data/ src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml --ignore=/Fixture/,/tests/_data/ src/ tests/",
        "phpstan": "phpstan analyse src/ --level=6",
        "test": [
            "codecept build",
            "codecept run"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "use-include-path": true,
        "process-timeout": 600,
        "sort-packages": true
    }
}
